xdotool search --name 'Firestone' windowactivate sleep 1
WID=$(xdotool getactivewindow)
xdotool sleep 1
xdotool mousemove --window $WID 519 1044 sleep 0.5 click --window $WID 1 
xdotool mousemove --window $WID 962 962 sleep 0.5 click --window $WID 1
xdotool sleep 50
xdotool mousemove --window $WID 3 160 sleep 0.5 click --window $WID 1
xdotool mousemove --window $WID 1022 634 sleep 0.5 click --window $WID 1 
xdotool mousemove --window $WID 630 962 sleep 0.5 click --window $WID 1
xdotool sleep 50
xdotool mousemove --window $WID 3 160 sleep 0.5 click --window $WID 1
xdotool sleep 0.5
xdotool mousemove --window $WID 1839 55 sleep 0.5 click --window $WID --repeat 4 --delay 300 1
xdotool mousemove --window $WID 3 160 sleep 0.5 click --window $WID 1 sleep 0.5
