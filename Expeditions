xdotool search --name 'Firestone' windowactivate sleep 1
WID=$(xdotool getactivewindow)
xdotool sleep 1
xdotool mousemove --window $WID 1856 469 sleep 0.5 click --window $WID 1
xdotool sleep 0.5
xdotool mousemove --window $WID 334 376 sleep 0.5 click --window $WID 1
xdotool sleep 0.5
xdotool mousemove --window $WID 1325 317 sleep 0.5 click --window $WID --repeat 2 --delay 200 1
xdotool mousemove --window $WID 1839 55 sleep 0.5 click --window $WID --repeat 4 --delay 300 1
xdotool mousemove --window $WID 3 160 sleep 0.5 click --window $WID 1 sleep 0.5